{
  "name": "OneWallet",
  "private": true,
  "version": "0.8.0",
  "description": "chrome extension oneWallet",
  "type": "module",
  "scripts": {
    "predev:chrome": "rimraf dist/chrome/dev",
    "dev:chrome": "cross-env NODE_OPTIONS=--max-old-space-size=12288 concurrently \"npm run dev:chrome:main\" \"npm run dev:chrome:inject\" \"npm run dev:chrome:content\" \"npm run dev:chrome:service-worker\"",
    "dev:chrome:serial": "cross-env NODE_OPTIONS=--max-old-space-size=12288 npm run dev:chrome:main && npm run dev:chrome:inject && npm run dev:chrome:content && npm run dev:chrome:service-worker",
    "dev:chrome:main": "tsc -b && vite build --config vite.config.chrome.ts --mode development",
    "dev:chrome:inject": "tsc -b && vite build --config vite.config.chrome.inject.ts --mode development",
    "dev:chrome:content": "tsc -b && vite build --config vite.config.chrome.content.ts --mode development",
    "dev:chrome:service-worker": "tsc -b && vite build --config vite.config.chrome.service-worker.ts --mode development",
    "prebuild:chrome": "rimraf dist/chrome/prod",
    "build:chrome": "concurrently \"npm run build:chrome:main\" \"npm run build:chrome:inject\" \"npm run build:chrome:content\" \"npm run build:chrome:service-worker\"",
    "build:chrome:main": "tsc -b && vite build --config vite.config.chrome.ts --mode production",
    "build:chrome:inject": "tsc -b && vite build --config vite.config.chrome.inject.ts --mode production",
    "build:chrome:content": "tsc -b && vite build --config vite.config.chrome.content.ts --mode production",
    "build:chrome:service-worker": "tsc -b && vite build --config vite.config.chrome.service-worker.ts --mode production",
    "postbuild:chrome": "mkdir -p release/chrome/zip && cd dist/chrome/prod && zip -r ../../../release/chrome/zip/extension-chrome-$(date +'%Y%m%dT%H%M%S').zip .",
    "scan:i18n": "i18next-scanner --config i18next-scanner.config.cjs",
    "lint": "eslint .",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version"
  },
